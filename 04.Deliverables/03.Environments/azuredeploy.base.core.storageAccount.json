{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "storageAccountResourceMode": {
            "type": "string",
            "minLength": 1,
            "defaultValue": "Incremental",
            "allowedValues": [
                "Incremental"
            ]
        },
        "storageAccountResourceName": {
            "type": "string",
            "minLength": 4
        },
        "storageAccountResourceLocation": {
            "type": "string",
            "allowedValues": [
                "Australia East",
                "Australia Southeast"
            ],
            "defaultValue": "[resourceGroup().location]"
        },
        "storageAccountResourceSku": {
            "type": "string",
            "allowedValues": [
                "Storage",
                "StorageV2",
                "BlobStorage"
            ],
            "defaultValue": "Storage"
        },
        "storageAccountKind": {
            "type": "string"
        }
    },
    "variables": {
        "resource": {
            "comment": "A collection of variables for use within this template (and any linked child templates).",
            "resourceNamePrefix": "azuredeploy.base.core"
        },
        "storageAccount": {
        }
    },
    "resources": [
    ],
    "outputs": {
        "resourceId": {
            "value": "[resourceId(variables('storageAccount').resourceType, variables('storageAccount').resourceName)]",
            "type": "string"
        }
    }
}